        <script type="text/javascript">
        $(document).ready(function(){	
            $('.add').on('click', add);
            $('.remove').on('click', remove);
        
        function add() {
            var new_chq_no = parseInt($('#total_chq').val()) + 1;
            var new_input = "<input type='text' id='new_" + new_chq_no + "'><br/>";
        var  new_input = "<p style='margin-top:0.05in;margin-right:0in;margin-bottom:.0001pt;margin-left:0.5in;line-height:115%;font-family:'Calibri','sans-serif';text-indent:.5in;'><strong><span style='line-height:115%;color:#222222;background:white;'><span class='input-new'><strng><input type='text' class='testing ka-naam-ankit' id='new_" + new_chq_no + "'></strong><span></span></strong><span style='font-family:'Arial','sans-serif';color:#222222;background:white;'>&nbsp;</span></p>"
            $('#new_chq').append(new_input);
            
            $('#total_chq').val(new_chq_no);
        }
        
        function remove() {
            var last_chq_no = $('#total_chq').val();
        
            if (last_chq_no > 1) {
            $('#new_' + last_chq_no).remove();
            $('#total_chq').val(last_chq_no - 1);
            }
        }
        
        $('.preview').on('click', function(){
            $('.add').hide()
            $('.remove').hide()
            $('.testing').each(function( index, ele ) {
                var valu = $(ele).val().replace(/^\s+|\s+$/gm,'');
                var paret = $(ele).parent().parent()
                paret.append("<span class='testing-valu'>"+ valu +"</span>")
                $(ele).hide()    
            });
        
            if($('.tal').val() === ''){
                $('#tal').remove()
            }
            if($('.prathm').val() === ''){
                $('#prathm').remove()
            }
            if($('.divitiya').val() === ''){
                $('#divitiya').remove()
            }
            if($('.tritiya').val() === ''){
                $('#tritiya').remove()
            }
            if($('.other').val() === ''){
            $('#other').remove()
            }
            
            var checkBox = $("#newAccountCheck");
            if (checkBox.is(":checked")){
            $('.new-account-label').show()
            checkBox.hide();
            } else {
            $('.new-account-label').hide()
            checkBox.hide();
            }
