            $('.edit').show()
            $('.final-preview').show()
            $('.final-save').hide()
        })

        $('.edit').on('click', function(){
            $('.preview').show();
            $('.testing-valu').remove()
            $('.testing').show()
            $(this).hide()
            $('.final-preview').hide()
            $('.final-save').show()
            $('.add').show()
            $('.remove').show()
            $("#newAccountCheck").show();
            $('.new-account-label').show();
        });
        
        $('.flat-no').on('blur', function(){
            $('.flat-no1').val($(this).val())
            $('.flat-no2').val($(this).val())
        })
        
        $('.mohalla').on('blur', function(){
            $('.mohalla1').val($(this).val())
            $('.mohalla2').val(`, ${$(this).val()}`)
        })
        
        $('.pavati').on('blur', function(){
            $('.pavati1').val($(this).val())
        })
        
        $('.c-pavati').on('blur', function(){
            $('.c-pavati1').val($(this).val())
        })

        $('.dinank').on('blur', function(){
            $('.dinank1').val($(this).val())
        })
        
        
        $('.book').on('blur', function(){
            $('.book1').val($(this).val())
        })
        
        $('#new_chq').on('blur', '.ka-naam-ankit', function(){
            var kaNaamAnkit = []
            $("#new_chq .ka-naam-ankit").each(function() {
                kaNaamAnkit.push($(this).val());
            });
            $('#ka-naam-ankit1').val(kaNaamAnkit.join(", "))
        })
        })

        $('.final-preview').on('click', function(){
            $('.edit').hide()
            $('.preview').hide()
            $(this).hide()
            window.print()
        });

        $('#newAccountCheck').on('click', function(){
            if ($(this).is(":checked")){
            $('.flat2-mohalla2').show()
            $('.no-flat2-mohalla2').hide()
            $('#new_account').val(true);
            } else {
            $('.flat2-mohalla2').hide()
            $('.no-flat2-mohalla2').show()
            $('#new_account').val(false);
            }
        });
        $('#newAccountCheck').prop('checked', <%= @namantaran.new_account %>);
      </script>
    </body>      
</html>